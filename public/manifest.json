{
    "author": "judeiwuji@gmail.com",
    "name": "Smart Mail - Gmail Copilot",
    "description": "Gmail automation with summarising and reply suggestions",
    "version": "0.1",
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "icons": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "key": "",
    "side_panel": {
        "default_path": "index.html"
    },
    "oauth2": {
        "client_id": "142654507053-2ilj16qv1ohhop119akiqetlgfr0sev3.apps.googleusercontent.com",
        "scopes": [
            "https://mail.google.com/"
        ]
    },
    "permissions": [
        "sidePanel",
        "activeTab",
        "tabs",
        "identity"
    ],
    "host_permissions": [
        "https://mail.google.com/"
    ],
    "manifest_version": 3,
    "web_accessible_resources": [
        {
            "resources": [
                "*.map"
            ],
            "matches": [
                "https://mail.google.com/*"
            ]
        }
    ]
}
